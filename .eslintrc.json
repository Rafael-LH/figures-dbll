{
"env":{
    "browser": true,
    "es6": true
},
"extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:react/recommended"
],
"globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "$": "readonly",
    "swal": "readonly",
    "google": "readonly"
},
"parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
},
"rules": {
    "no-shadow": "off",
    "import/extensions": "off",
    "no-plusplus": "off",
    "max-len": "off"
  }
}